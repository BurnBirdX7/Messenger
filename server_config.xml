<config>
    <ssl version="sslv23"> <!-- version is ignored by now -->
        <cert>server.pem</cert>
        <key asn1="false">server_key.pem</key>
        <dh>dh2048.pem</dh>
        <options>
            <option>default_workarounds</option>
            <option>no_sslv2</option>
            <option>single_dh_use</option>
        </options>
    </ssl>
    <port>56666</port>

    <!-- threads: numbers lower than 1 or greater than 100 will be ignored and default value will be set -->
    <io_threads>2</io_threads>
</config>